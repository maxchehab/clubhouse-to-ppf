{
  "name": "@maxchehab/clubhouse-to-ppf",
  "version": "0.0.2",
  "files": [
    "dist/**/*",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/bin"
  ],
  "bin": {
    "ppf": "./bin/run"
  },
  "oclif": {
    "commands": "./dist/commands",
    "bin": "ppf",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "clean": "rm -rf dist",
    "prettier": "prettier \"src/**/*.{js,ts,tsx}\" --check",
    "format": "prettier \"src/**/*.{js,ts,tsx}\" --write",
    "prepare": "yarn run build",
    "bump-version": " auto-upgrade-npm-version",
    "prepack": "rm -rf dist && tsc -b && oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "prepublishOnly": "yarn run lint",
    "preversion": "yarn run lint"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.0",
    "@types/chalk": "2.2.0",
    "@types/cli-progress": "1.8.1",
    "@types/faker": "4.1.5",
    "@types/inquirer": "^6.0.3",
    "@types/jest": "24.0.14",
    "@types/node": "12.0.8",
    "auto-upgrade-npm-version": "1.0.7",
    "globby": "9.2.0",
    "jest": "24.8.0",
    "mock-stdin": "0.3.1",
    "prettier": "1.18.2",
    "stdout-stderr": "0.1.9",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "tslint": "5.17.0",
    "typescript": "3.5.1"
  },
  "description": "generates a ppf from stories created in clubhouse",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxchehab/clubhouse-to-ppf.git"
  },
  "keywords": [
    "clubhouse",
    "ppf"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxchehab/clubhouse-to-ppf/issues"
  },
  "homepage": "https://github.com/maxchehab/clubhouse-to-ppf#readme",
  "dependencies": {
    "@oclif/command": "1.5.15",
    "@oclif/config": "1.13.0",
    "@oclif/plugin-help": "2.2.0",
    "chalk": "2.4.2",
    "cli-progress": "2.1.1",
    "cli-ux": "5.3.0",
    "clipboardy": "^2.1.0",
    "clubhouse-lib": "^0.6.0",
    "faker": "4.1.0",
    "inquirer": "^6.4.1",
    "ts-node": "^8.3.0",
    "tslib": "1.10.0"
  }
}
